
<form asp-controller="Login" asp-action="RegisterAsync">
    <h1>Register</h1>
    <p>Username:</p>
    <input type="text" name="Username" />
    <p>Password:</p>
    <input type="text" name="Password" />
    <p>Email:</p>
    <input type="text" name="Email" />
    <input type="submit" placeholder="Register" />
</form>
